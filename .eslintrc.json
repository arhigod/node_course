{
  "env": {
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "plugins": ["prettier"],
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "linebreak-style": ["error", "windows"]
  }
}
